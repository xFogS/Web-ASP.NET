@model Web_GEO.Models.Cars_Tayota.ColorModel

<h1>Details</h1>

<dl class="row">
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Name)
    </dt>
    <dd class="col-sm-10">
        @ViewBag.modelOfTheCar.Name
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.URL)
    </dt>
    <dd class="col-sm-10">
        <img src="@ViewBag.modelOfTheCar.URL" height="300" width="300" alt="@ViewBag.modelOfTheCar.URL" style="border-radius:10px" />
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.RGB)
    </dt>
    <dd class="col-sm-10">
        @ViewBag.modelOfTheCar.RGB
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Code)
    </dt>
    <dd class="col-sm-10">
        @ViewBag.modelOfTheCar.Code
    </dd>
</dl>


<div class="form-group"><a asp-action="Edit" asp-route-id="@ViewBag.modelOfTheCar.Id">Edit</a></div>
<div class="form-group"><a asp-action="Index">Back To List</a></div>


